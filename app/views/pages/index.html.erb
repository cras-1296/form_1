<h1> Form 4.0</h1>
<form method="get" action="pages/create">
    <div class="form-group">
        <label for="email">Email address</label>
        <input name="email" type="email" class="form-control" id="email"
        aria-describedby="emailHelp" placeholder="Ingrese email">
        <small id="emailHelp" class="form-text text-muted">Nunca compartiremos tu correo con nadie.</small>
    </div>
    <div class="form-group">
        <label for="age">Edad</label>
        <input name="age" class="form-control">
    </div>
    <div class="form-group">
        <label for="comment">Comentario</label>
        <textarea name="comment" class="form-control"> </textarea>
    </div>
    <br>
    <button type="submit" class="btn btn-primary">Enviar</button>
</form>
